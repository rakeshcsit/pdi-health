<h1>Listing inrs</h1>

<%= link_to 'Log an INR', new_inr_path %>

<table class="table">
  <thead>
    <tr>
      <th>INR</th>
      <th>Note</th>
      <th>Date of INR</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @inrs.each do |inr| %>
      <tr>
        <td><%= inr.value %></td>
        <td><%= inr.note %></td>
        <td><%= inr.wdate %></td>
        <td><%= link_to 'Show', inr %></td>
        <td><%= link_to 'Edit', edit_inr_path(inr) %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

