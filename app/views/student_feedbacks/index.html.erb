<h1>Listing student_feedbacks</h1>

<table>
  <thead>
    <tr>
      <th>Class code</th>
      <th>Week</th>
      <th>Student name</th>
      <th>How class week</th>
      <th>Pace</th>
      <th>Acad supp</th>
      <th>New learn</th>
      <th>Mastery</th>
      <th>Inst engag</th>
      <th>Inst clarity</th>
      <th>Inst know</th>
      <th>Hours outside</th>
      <th>Spend time on</th>
      <th>Pace notes</th>
      <th>Support notes</th>
      <th>Inst notes</th>
      <th>Add notes</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @student_feedbacks.each do |student_feedback| %>
      <tr>
        <td><%= student_feedback.class_code %></td>
        <td><%= student_feedback.week %></td>
        <td><%= student_feedback.student_name %></td>
        <td><%= student_feedback.how_class_week %></td>
        <td><%= student_feedback.pace %></td>
        <td><%= student_feedback.acad_supp %></td>
        <td><%= student_feedback.new_learn %></td>
        <td><%= student_feedback.mastery %></td>
        <td><%= student_feedback.inst_engag %></td>
        <td><%= student_feedback.inst_clarity %></td>
        <td><%= student_feedback.inst_know %></td>
        <td><%= student_feedback.hours_outside %></td>
        <td><%= student_feedback.spend_time_on %></td>
        <td><%= student_feedback.pace_notes %></td>
        <td><%= student_feedback.support_notes %></td>
        <td><%= student_feedback.inst_notes %></td>
        <td><%= student_feedback.add_notes %></td>
        <td><%= link_to 'Show', student_feedback %></td>
        <td><%= link_to 'Edit', edit_student_feedback_path(student_feedback) %></td>
        <td><%= link_to 'Destroy', student_feedback, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Student feedback', new_student_feedback_path %>
