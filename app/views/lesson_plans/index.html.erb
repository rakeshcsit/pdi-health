<h1>Listing lesson_plans</h1>

<%= link_to 'New Lesson plan', new_lesson_plan_path %>

<br><br>

<table class="table">
  <thead>
    <tr>
      <th>Lp</th>
      <th>Lp link</th>
      <th></th>
    </tr>
  </thead>

  <tbody>
    <% @lesson_plans.each do |lesson_plan| %>
      <tr>
        <td><%= link_to lesson_plan.lp, lesson_plan %></td>
        <td><%= lesson_plan.lp_link %></td>
        <td><%= link_to 'Edit', edit_lesson_plan_path(lesson_plan) %></td>
      </tr>
    <% end %>
  </tbody>
</table>
