<h1>Listing rds</h1>

<%= link_to 'New Rd', new_rd_path %>
<br>

<table class="table">
  <thead>
    <tr>
      <th>Rd</th>
      <th>Rd email</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @rds.each do |rd| %>
      <tr>
        <td><%= rd.rd %></td>
        <td><%= rd.rd_email %></td>
        <td><%= link_to 'Show', rd %></td>
        <td><%= link_to 'Edit', edit_rd_path(rd) %></td>
      </tr>
    <% end %>
  </tbody>
</table>
