<h1>Listing Student Success Managers</h1>

<%= link_to 'New Student success manager', new_student_success_manager_path %>

<br><br>

<table class="table">
  <thead>
    <tr>
      <th>University</th>
      <th>Ss</th>
      <th>Ss email</th>
      <th></th>
    </tr>
  </thead>

  <tbody>
    <% @student_success_managers.each do |student_success_manager| %>
      <tr>
        <td><%= link_to student_success_manager.university.university,  student_success_manager.university %></td>
        <td><%= link_to student_success_manager.ss, student_success_manager %></td>
        <td><%= student_success_manager.ss_email %></td>
        <td><%= link_to 'Edit', edit_student_success_manager_path(student_success_manager) %></td>
      </tr>
    <% end %>
  </tbody>
</table>
